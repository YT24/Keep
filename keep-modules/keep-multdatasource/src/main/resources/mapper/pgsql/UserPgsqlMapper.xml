<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.keep.multdatasource.mapper.pgsql.UserPgsqlMapper">
    <select id="getById" resultType="com.keep.multdatasource.entity.UserPgsql">
        select *
        from user_pgsql
        where id = #{id}
    </select>
</mapper>
